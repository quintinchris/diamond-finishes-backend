    "typescript": "~3.9.7",
    "webpack": "^5.70.0",
    "webpack-cli": "^4.9.2"
  },
  "dependencies": {
    "@types/js-yaml": "^4.0.5",
    "aws-cdk-lib": "2.15.0",
    "aws-lambda": "^1.0.7",
    "constructs": "^10.0.0",
    "fork-ts-checker-webpack-plugin": "^7.2.3",
